<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="196.0" prefWidth="380.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.ExitController">
    <children>
        <Button fx:id="yes_Btn" layoutX="99.0" layoutY="131.0" mnemonicParsing="false" onAction="#continueExit" prefHeight="34.0" prefWidth="72.0" text="Yes" textAlignment="CENTER" />
        <Button fx:id="cancel_btn" layoutX="203.0" layoutY="131.0" mnemonicParsing="false" onAction="#abortExit" prefHeight="34.0" prefWidth="72.0" text="Cancel" textAlignment="CENTER" />
        <TextField alignment="CENTER" layoutX="81.0" layoutY="54.0" prefHeight="44.0" prefWidth="217.0" style="-fx-background-color: gray;" text="Are you sure that you want to Exit?">
            <font>
                <Font name="System Bold" size="12.0" />
            </font></TextField>
    </children>
</AnchorPane>