<AnchorPane prefHeight="173.0" prefWidth="320.0" stylesheets="@Exit.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.ExitController">
    <children>
        <Button fx:id="yes_Btn" layoutX="89.0" layoutY="127.0" mnemonicParsing="false" onAction="#continueExit" prefHeight="25.0" prefWidth="60.0" text="Yes" textAlignment="CENTER" />
        <Button fx:id="cancel_btn" layoutX="171.0" layoutY="127.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="60.0" text="Cancel" textAlignment="CENTER" />
        <TextField alignment="CENTER" layoutX="51.0" layoutY="14.0" prefHeight="22.0" prefWidth="217.0" style="-fx-background-color: white;" text="Are you sure that you want to Exit?">
            <font>
                <Font name="System Bold Italic" size="12.0" />
            </font></TextField>
      <ImageView fitHeight="78.0" fitWidth="161.0" layoutX="80.0" layoutY="42.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@/images/Ursula2.png" />
         </image>
      </ImageView>
    </children>
</AnchorPane>
